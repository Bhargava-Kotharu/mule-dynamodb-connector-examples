#%RAML 1.0 DataType
properties:
  id: 
    type: number
    required: false
    example: 123
  firstName:
    type: string
    required: false
    example: "Bhargava"
  lastName:
    type: string
    required: false
    example: "Bhargava"  
  email:
    type: string
    required: false
    example: "bhargava.kotharu@gmail.com"
  address:
    type: string
    required: false
    example: "London, United Kingdom"